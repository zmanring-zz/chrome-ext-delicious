{

 "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["vendor/jquery-1.9.0.min.js","inject.js"],
    "css": ["background.css"]
    }
  ],
  "description": "Chrome extension with context menu that adds links and manages bookmarks from your Delicious account.",
  "icons": {
    "16": "images/icon-16.png",
    "128":"images/icon-128.png"
  },
  "manifest_version": 2,
  "name": "@Delicious",
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "https://api.del.icio.us/",
    "tabs"
  ],
  "version": "1.3",
  "web_accessible_resources": [ "popup.html" ]

}
